include ../mixins/_mixin
doctype html
html(lang='en')
	head
		meta(charset='utf-8')
		meta(name='viewport', content='width=device-width, initial-scale=1.0, viewport-fit=cover')
		title ProjectTitle
		link(media='all', rel='stylesheet', href='style.css')
		script(src="js/jquery-3.6.0.min.js", defer)
		script(src='js/jquery.main.js', defer)
		script(src="https://cdnjs.cloudflare.com/ajax/libs/flexslider/2.6.1/jquery.flexslider.min.js", defer)

	//- -var bodyClass= "home"; // add class on body
	body(class= bodyClass)
		//- -var wrapperClass= "className"; // add class on #wrapper
		#wrapper(class= wrapperClass)
			block content
